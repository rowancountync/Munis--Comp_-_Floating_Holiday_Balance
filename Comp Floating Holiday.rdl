        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>85170a72-6f80-4a0b-8660-e0ea7c837f45</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Accruals">
      <Query>
        <DataSourceName>TylerCI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Location">
            <Value>=Parameters!Location.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT 
                         em.a_location_primary + ' - ' + em.a_location_p_short AS Location, em.a_name_last + ', ' + em.a_name_first AS EmplName, em.a_employee_number, ac.a_accrual_type, 
                         (CASE WHEN a_accrual_type = 'C' THEN 'COMP EARNED' WHEN a_accrual_type = 'H' THEN 'FLOATING HOLIDAY' END) AS AccrualDescr, ac.ac_active_y_n, ac.ac_quant_avail
FROM            pr_accruals AS ac LEFT OUTER JOIN
                         pr_employee_mast AS em ON ac.a_employee_number = em.a_employee_number
WHERE        (em.a_location_primary IN (@Location)) AND (ac.a_accrual_type IN ('C', 'H')) AND (ac.ac_quant_avail &gt; 0) AND (ac.ac_active_y_n = 'Y')
ORDER BY Location, EmplName</CommandText>
      </Query>
      <Fields>
        <Field Name="Location">
          <DataField>Location</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmplName">
          <DataField>EmplName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="a_employee_number">
          <DataField>a_employee_number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="a_accrual_type">
          <DataField>a_accrual_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AccrualDescr">
          <DataField>AccrualDescr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ac_active_y_n">
          <DataField>ac_active_y_n</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ac_quant_avail">
          <DataField>ac_quant_avail</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="LocationDS">
      <Query>
        <DataSourceName>TylerCI</DataSourceName>
        <CommandText>SELECT DISTINCT a_location_primary, a_location_primary + ' - ' + a_location_p_short AS Location
FROM            pr_employee_mast
WHERE        (a_location_primary &lt;&gt; '4110') AND (a_location_primary &lt;&gt; '9000')
ORDER BY a_location_primary</CommandText>
      </Query>
      <Fields>
        <Field Name="a_location_primary">
          <DataField>a_location_primary</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Location">
          <DataField>Location</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
